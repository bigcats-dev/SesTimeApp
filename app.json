{
  "expo": {
    "name": "SesTimeApp",
    "slug": "SesTimeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashx.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
